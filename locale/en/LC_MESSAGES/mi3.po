msgstr ""

#: ../../source/mi3.rst:795
msgid "4 tornillos M3x10mm"
msgstr ""

#: ../../source/mi3.rst:796
msgid "2 tornillos cabeza plana M3x10mm"
msgstr ""

#: ../../source/mi3.rst:797
msgid "4 tornillos cabeza plana M3x35mm"
msgstr ""

#: ../../source/mi3.rst:798
msgid "27 tuercas M3 de seguridad"
msgstr ""

#: ../../source/mi3.rst:799
msgid "2 tuercas M3"
msgstr ""

#: ../../source/mi3.rst:800 ../../source/mi3.rst:1495
msgid "2 tuercas 5/16"
msgstr ""

#: ../../source/mi3.rst:801
msgid "27 rondanas M3"
msgstr ""

#: ../../source/mi3.rst:802
msgid "4 piezas plasticas D"
msgstr ""

#: ../../source/mi3.rst:803
